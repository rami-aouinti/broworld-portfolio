<h5>{{ 'menu.experience'|trans|upper }}</h5>

{% for experience in experiences %}
<table class="first" cellpadding="4" cellspacing="6">
    <tr>
        <td width="370" height="100">
            <h6>{{ experience.title }}</h6>
            <h6> {{ experience.company }}</h6>
        </td>
        <td width="100" height="100">
            <br><br><br><br>
            {{ experience.startedAt.format("Y") }} - {{experience.endedAt == null ? "Now" : experience.endedAt.format("Y")}}
        </td>

    </tr>
</table>
<br><br>
{{experience.description|markdown_to_html|sanitize_html}}
{% endfor %}
